{
	"$schema": "vscode://schemas/color-theme",
	"include": "./dark_defaults.json",
	"colors": {
		"editor.foreground": "#EFEFEF",
		"editorCursor.foreground": "#FFFFFF",
		"panelTitle.activeBorder": "#BFBFBF",
		"panelTitle.activeForeground": "#BFBFBF",
		"panelTitle.inactiveForeground": "#BFBFBF99",
		"sideBarTitle.foreground": "#BFBFBF",
		"terminal.ansiBrightWhite": "#EFEFEF", /* same as editor.foreground */
		"terminal.ansiWhite": "#BFBFBF",
		"terminal.foreground": "#BFBFBF", /* same as ansiWhite */
		"terminalCursor.foreground": "#FFFFFF"
	},
	"tokenColors": [
		/* comment */
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8F80BF"
			}
		},
		{
			"name": "Comment for document generator",
			"scope": [
				"comment.block.documentation"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#809FBF"
			}
		},
		{
			"name": "Comment symbol",
			"scope": [
				"punctuation.definition.comment" /* not in the schema */
			],
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		/* keyword */
		{
			"name": "Keyword",
			"scope": [
				"entity.name.tag",
				"keyword",
				"keyword.operator.alignas",
				"keyword.operator.alignof",
				"keyword.operator.cast",
				"keyword.operator.delete",
				"keyword.operator.expression",
				"keyword.operator.functionlike",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.new",
				"keyword.operator.noexcept",
				"keyword.operator.sizeof",
				"keyword.operator.typeid",
				"keyword.operator.wordlike",
			],
			"settings": {
				"foreground": "#E971DA"
			}
		},
		/* operator */
		{
			"name": "Language-defined operator",
			"scope": [
				"keyword.operator",
				"punctuation.separator.delimiter", /* C/C++ */
				"punctuation.terminator.statement", /* C/C++ */
				"punctuation.section.parens.begin.bracket.round", /* C/C++ */
				"punctuation.section.parens.end.bracket.round", /* C/C++ */
				"punctuation.section.parameters.begin.bracket.round", /* C/C++ */
				"punctuation.section.parameters.end.bracket.round", /* C/C++ */
				"punctuation.section.arguments.begin.bracket.round", /* C/C++ */
				"punctuation.section.arguments.end.bracket.round", /* C/C++ */
				"punctuation.definition.begin.bracket.square", /* C/C++ */
				"punctuation.definition.end.bracket.square", /* C/C++ */
				"punctuation.section.block.begin.bracket.curly", /* C/C++ */
				"punctuation.section.block.end.bracket.curly" /* C/C++ */
			],
			"settings": {
				"foreground": "#BFBFBF"
			}
		},
		{
			"name": "User-defined operator",
			"scope": [
				"entity.other",
				"entity.other.infix" /* not in the schema */
			],
			"settings": {
				"foreground": "#BFCFDF"
			}
		},
		{
			"name": "User-defined prefix operator",
			"scope": [
				"entity.other.prefix"
			],
			"settings": {
				"foreground": "#DFBFCF"
			}
		},
		{
			"name": "User-defined indexing operator",
			"scope": [
				"entity.other.indexing"
			],
			"settings": {
				"foreground": "#CFDFBF"
			}
		},
		/* attribute */
		{
			"name": "Language-defined attribute",
			"scope": [
				"entity.other.attribute-name.defined" /* not in the schema */
			],
			"settings": {
				"foreground": "#ECB4B4"
			}
		},
		{
			"name": "User-defined attribute",
			"scope": [
				"entity.name.tag.restructuredtext",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ECECB4"
			}
		},
		/* directive */
		{
			"name": "Directive",
			"scope": [
				"keyword.other",
				"keyword.control.directive", /* not in the schema */
				"meta.other.directive"
			],
			"settings": {
				"foreground": "#E97171"
			}
		},
		/* entity */
		{
			"name": "Entity",
			"scope": [
				"entity",
				"variable"
			],
			"settings": {
				"foreground": "#EFEFEF" /* same as foreground */
			}
		},
		/* literal */
		{
			"name": "Literal",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#EFEF8E"
			}
		},
		{
			"name": "Escaped sequence in literal",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#EF2F2F"
			}
		},
		{
			"name": "String literal",
			"scope": [
				"entity.name.directive.restructuredtext",
				"string"
			],
			"settings": {
				"foreground": "#8EEFEF"
			}
		},
		/* markuped text */
		{
			"name": "Text",
			"scope": [
				"invalid.deprecated.line-too-long.git-commit", /* git-commit */
				"markup.table",
				"punctuation.definition.list_item.markdown", /* markdown */
				"punctuation.section.class.end", /* not in the schema */
				"string.other.footnote", /* not in the schema */
				"text.html.markdown", /* markdown */
			],
			"settings":{
				"foreground": "#EFEFEF" /* same as foreground */
			}
		},
		{
			"name": "Raw text",
			"scope": [
				"markup.raw"
			],
			"settings":{
				"foreground": "#80E080"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Bold italic",
			"scope": [
				"markup.bold markup.italic",
				"markup.italic markup.bold"
			],
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		/* link */
		{
			"name": "Link identifier",
			"scope": [
				"constant.other.footnote.link" /* not in the schema */
			],
			"settings": {
				"foreground": "#E08080"
			}
		},
		{
			"name": "Link title",
			"scope": [
				"string.other.link.title" /* not in the schema */
			],
			"settings": {
				"foreground": "#8080E0"
			}
		},
		{
			"name": "Link URI",
			"scope": [
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#E0E080"
			}
		},
		/* markup */
		{
			"name": "Paragraph-level markup symbol",
			"scope": [
				"punctuation.definition.heading", /* not in the schema */
				"punctuation.section.raw" /* not in the schema */
			],
			"settings": {
				"foreground": "#3FBFBF"
			}
		},
		{
			"name": "Inline-level markup symbol",
			"scope": [
				"punctuation.definition.bold", /* not in the schema */
				"punctuation.definition.constant", /* not in the schema */
				"punctuation.definition.italic", /* not in the schema */
				"punctuation.definition.link", /* not in the schema */
				"punctuation.definition.raw" /* not in the schema */
			],
			"settings": {
				"foreground": "#BF3FBF"
			}
		},
		{
			"name": "Definition markup symbol",
			"scope": [
				"meta.link.footnote.def punctuation.definition.constant", /* not in the schema */
				"meta.link.reference.def punctuation.definition.string" /* not in the schema */
			],
			"settings": {
				"foreground": "#C0C0C0"
			}
		},
		/* diff headers */
		{
			"name": "Header",
			"scope": [
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#EFEF80"
			}
		},
		{
			"name": "Line number",
			"scope": [
				"meta.diff.range"
			],
			"settings": {
				"foreground": "#8080EF"
			}
		},
		/* diff */
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#80EF80"
			}
		},
		{
			"name": "Inserted in comment",
			"scope": [
				"comment markup.inserted"
			],
			"settings": {
				"foreground": "#88BEA2" /* average of Inserted and Comment */
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#EF8080"
			}
		},
		{
			"name": "Deleted in comment",
			"scope": [
				"comment markup.deleted"
			],
			"settings": {
				"foreground": "#C480A2" /* average of Deleted and Comment */
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#EFEF80"
			}
		},
		{
			"name": "Changed in comment",
			"scope": [
				"comment markup.changed"
			],
			"settings": {
				"foreground": "#C4BEA2" /* average of Changed and Comment */
			}
		},
		/* unmodified */
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"variable.function",
				"support.function"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"support.constant",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"markup.heading",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"support.other.namespace.php",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "JSON Key - Level 0",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "JSON Key - Level 1",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "JSON Key - Level 2",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "JSON Key - Level 3",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "JSON Key - Level 4",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#C17E70"
			}
		},
		{
			"name": "JSON Key - Level 5",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "JSON Key - Level 6",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "JSON Key - Level 7",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "JSON Key - Level 8",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Markdown - Markup Raw Inline",
			"scope": [
				"text.html.markdown markup.inline.raw.markdown"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Markdown - Markup Raw Inline Punctuation",
			"scope": [
				"text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
			],
			"settings": {
				"foreground": "#65737E"
			}
		},
		{
			"name": "Markdown - Line Break",
			"scope": [
				"text.html.markdown meta.dummy.line-break"
			],
			"settings": {}
		},
		{
			"name": "Markdown - Heading",
			"scope": [
				"markdown.heading",
				"markup.heading | markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Markup - Bold-Italic",
			"scope": [
				"markup.quote markup.bold",
				"markup.quote markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f07178"
			}
		},
		{
			"name": "Markup - Strike",
			"scope": [
				"markup.strike"
			],
			"settings": {
				"fontStyle": "italic underline"
			}
		},
		{
			"name": "Markdown - Blockquote",
			"scope": [
				"markup.quote punctuation.definition.blockquote.markdown"
			],
			"settings": {
				"foreground": "#65737E"
			}
		},
		{
			"name": "Markup - Quote",
			"scope": [
				"markup.quote"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown - Fenced Bode Block",
			"scope": [
				"punctuation.definition.fenced.markdown"
			],
			"settings": {
				"foreground": "#00000050"
			}
		},
		{
			"name": "Markdown - Fenced Language",
			"scope": [
				"variable.language.fenced.markdown"
			],
			"settings": {
				"foreground": "#65737E"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	]
}
