{
	"$schema": "vscode://schemas/color-theme",
	"include": "./light_vs.json",
	"colors": {
		"activityBar.background": "#333333",
		"breadcrumb.activeSelectionForeground": "#4e4e4e",
		"breadcrumb.focusForeground": "#4e4e4e",
		"breadcrumb.foreground": "#616161cc",
		"editor.foreground": "#000000",
		"editorCursor.foreground": "#000000",
		"editorHoverWidget.foreground": "#616161",
		"editorLink.activeForeground": "#0000AE", /* L=5/12 */
		"editorWidget.foreground": "#616161",
		"gitDecoration.addedResourceForeground": "#81B88B",
		"gitDecoration.deletedResourceForeground": "#C74E39",
		"gitDecoration.ignoredResourceForeground": "#8C8C8C",
		"gitDecoration.modifiedResourceForeground": "#E2C08D",
		"gitDecoration.submoduleResourceForeground": "#8DB9E2",
		"gitDecoration.untrackedResourceForeground": "#73C991",
		"input.foreground": "#000000",
		"list.activeSelectionBackground": "#094771", /* dark */
		"list.dropBackground": "#383B3D", /* dark */
		"list.errorForeground": "#F88070", /* dark */
		"list.focusBackground": "#566BFF66", /* light/dark */
		"list.focusHighlightForeground": "#9dddff",
		"list.highlightForeground": "#0066bf",
		"list.hoverBackground": "#7F7F7F80", /* light/dark */
		"list.inactiveSelectionBackground": "#37373D", /* dark */
		"list.warningForeground": "#CCA700", /* dark */
		"menu.foreground": "#616161",
		"notifications.foreground": "#616161",
		"panel.background": "#DFE3E7", /* same as editorGroupHeader.tabsBackground */
		"panelTitle.activeBorder": "#C5C5C5", /* same as tab.border */
		"panelTitle.activeForeground": "#333333", /* same as tab.activeForeground */
		"panelTitle.inactiveForeground": "#333333B3", /* same as tab.inactiveForeground */
		"problemsErrorIcon.foreground": "#F48771",
		"problemsWarningIcon.foreground": "#CCA700",
		"quickInput.background": "#F3F3F3",
		"quickInput.foreground": "#616161",
		"scrollbar.shadow": "#000000",
		"scrollbarSlider.activeBackground": "#BFBFBF66",
		"scrollbarSlider.background": "#79797966",
		"sideBar.dropBackground": "#FFFFFF1F",
		"sideBar.foreground": "#CCCCCC",
		"sideBarTitle.foreground": "#BFBFBF",
		"tab.border": "#C5C5C5",
		"tab.inactiveBackground": "#DFE3E7",
		"terminal.ansiBlue": "#4343FF",
		"terminal.ansiBrightBlue": "#6868FF",
		"terminal.ansiBrightCyan": "#00EEEE",
		"terminal.ansiBrightGreen": "#00EC00",
		"terminal.ansiBrightMagenta": "#FF26FF",
		"terminal.ansiBrightRed": "#FF2E2E",
		"terminal.ansiBrightWhite": "#E5E5E5",
		"terminal.ansiBrightYellow": "#E3E300",
		"terminal.ansiCyan": "#00C6C6",
		"terminal.ansiGreen": "#00C500",
		"terminal.ansiMagenta": "#D800D8",
		"terminal.ansiRed": "#DB0000",
		"terminal.ansiWhite": "#BFBFBF",
		"terminal.ansiYellow": "#BDBD00",
		"terminal.foreground": "#D3D3D3", /* middle of ansiWhite and ansiBrightWhite */
		"terminal.selectionBackground": "#FFFFFF40",
		"terminalCursor.foreground": "#FFFFFF",
		"tree.indentGuidesStroke": "#585858",
		"widget.shadow": "#000000",
	},
	"tokenColors": [
		/* comment */
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9C74A8"
			}
		},
		{
			"name": "Comment for document generator",
			"scope": [
				"comment.block.documentation"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A8748F"
			}
		},
		{
			"name": "Comment symbol",
			"scope": [
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		/* keyword */
		{
			"name": "Keyword",
			"scope": [
				"entity.name.tag",
				"keyword",
				"keyword.operator.alignas",
				"keyword.operator.alignof",
				"keyword.operator.cast",
				"keyword.operator.delete",
				"keyword.operator.expression",
				"keyword.operator.functionlike",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.new",
				"keyword.operator.noexcept",
				"keyword.operator.sizeof",
				"keyword.operator.typeid",
				"keyword.operator.wordlike",
			],
			"settings": {
				"foreground": "#2A3DB9" /* L=1/2 */
			}
		},
		/* operator */
		{
			"name": "Language-defined operator",
			"scope": [
				"keyword.operator",
				"punctuation.separator.delimiter",
				"punctuation.terminator.statement",
				"punctuation.section.parens.begin.bracket.round",
				"punctuation.section.parens.end.bracket.round",
				"punctuation.section.parameters.begin.bracket.round",
				"punctuation.section.parameters.end.bracket.round",
				"punctuation.section.arguments.begin.bracket.round",
				"punctuation.section.arguments.end.bracket.round",
				"punctuation.definition.begin.bracket.square",
				"punctuation.definition.end.bracket.square",
				"punctuation.section.block.begin.bracket.curly",
				"punctuation.section.block.end.bracket.curly"
			],
			"settings": {
				"foreground": "#686868" /* L=1/3 */
			}
		},
		{
			"name": "User-defined operator / Inline-level",
			"scope": [
				"entity.other",
				"entity.other.infix",
				"punctuation.definition.bold",
				"punctuation.definition.constant",
				"punctuation.definition.italic",
				"punctuation.definition.link",
			],
			"settings": {
				"foreground": "#83475E" /* L=1/3 */
			}
		},
		{
			"name": "User-defined prefix operator / Paragraph-level",
			"scope": [
				"entity.other.prefix",
				"punctuation.definition.heading",
				"punctuation.definition.raw",
				"punctuation.section.raw",
				"meta.link.footnote.def punctuation.definition.constant",
				"meta.link.reference.def punctuation.definition.string",
			],
			"settings": {
				"foreground": "#475E83" /* L=1/3 */
			}
		},
		{
			"name": "User-defined indexing operator",
			"scope": [
				"entity.other.indexing"
			],
			"settings": {
				"foreground": "#5E8347" /* L=1/3 */
			}
		},
		/* attribute */
		{
			"name": "Language-defined attribute",
			"scope": [
				"entity.other.attribute-name.defined",
				"source.rst variable.parameter.codeblock"
			],
			"settings": {
				"foreground": "#168080" /* L=1/4 */
			}
		},
		{
			"name": "User-defined attribute",
			"scope": [
				"entity.other.attribute-name",
				"source.rst variable.parameter"
			],
			"settings": {
				"foreground": "#168016" /* L=1/4 */
			}
		},
		/* directive */
		{
			"name": "Directive",
			"scope": [
				"keyword.other",
				"keyword.control.directive",
				"meta.other.directive",
				"source.rst keyword.control"
			],
			"settings": {
				"foreground": "#2597BA" /* L=1/2 */
			}
		},
		/* entity */
		{
			"name": "Entity",
			"scope": [
				"entity",
				"variable"
			],
			"settings": {
				"foreground": "#000000" /* same as foreground */
			}
		},
		{
			"name": "Language-defined entity",
			"scope": [
				"support.class",
				"support.constant",
				"support.function",
				"support.type",
				"support.variable"
			],
			"settings": {
				"foreground": "#305959" /* L=1/4 */
			}
		},
		/* literal */
		{
			"name": "Literal",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#008300" /* L=1/4 */
			}
		},
		{
			"name": "Escaped sequence in literal",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#00AEAE" /* L=5/12 */
			}
		},
		{
			"name": "String literal",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#830000" /* L=1/4 */
			}
		},
		/* preprocessor */
		{
			"name": "Preprocessor text",
			"scope": [
				"meta.preprocessor",
				"meta.preprocessor entity",
				"meta.preprocessor variable",
			],
			"settings": {
				"foreground": "#474747" /* L=1/6 */
			}
		},
		/* markuped text */
		{
			"name": "Text",
			"scope": [
				"invalid.deprecated.line-too-long.git-commit",
				"markup.table",
				"punctuation.definition.list_item.markdown",
				"punctuation.section.class.end",
				"string.other.footnote",
				"text.html.markdown",
			],
			"settings":{
				"foreground": "#000000" /* same as foreground */
			}
		},
		{
			"name": "Raw text",
			"scope": [
				"markup.inline.raw",
				"markup.raw"
			],
			"settings":{
				"foreground": "#9D7640" /* L=5/12 */
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Bold italic",
			"scope": [
				"markup.bold markup.italic",
				"markup.italic markup.bold"
			],
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Head",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#474783" /* L=1/3 */
			}
		},
		/* link */
		{
			"name": "Link identifier",
			"scope": [
				"constant.other.footnote.link",
				"source.rst entity.name.tag"
			],
			"settings": {
				"foreground": "#7600AE" /* L=5/12 */
			}
		},
		{
			"name": "Link title",
			"scope": [
				"string.other.link.title"
			],
			"settings": {
				"foreground": "#0000AE" /* same as editorLink.activeForeground */
			}
		},
		{
			"name": "Link URI",
			"scope": [
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#767676" /* L=5/12 */
			}
		},
		/* diff headers */
		{
			"name": "Header",
			"scope": [
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#A8A800"
			}
		},
		{
			"name": "Line number",
			"scope": [
				"meta.diff.range"
			],
			"settings": {
				"foreground": "#00A8A8"
			}
		},
		/* diff */
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#00a800"
			}
		},
		{
			"name": "Inserted in comment",
			"scope": [
				"comment markup.inserted"
			],
			"settings": {
				"foreground": "#6A9072" /* average of Inserted and Comment */
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#A80000"
			}
		},
		{
			"name": "Deleted in comment",
			"scope": [
				"comment markup.deleted"
			],
			"settings": {
				"foreground": "#A24F72" /* average of Deleted and Comment */
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#A8A800"
			}
		},
		{
			"name": "Changed in comment",
			"scope": [
				"comment markup.changed"
			],
			"settings": {
				"foreground": "#A29072" /* average of Changed and Comment */
			}
		},
		/* rainbow */
		{
			"scope": [
				"rainbowgroup rainbow1"
			],
			"settings": {
				"foreground": "#474747", /* L=1/6 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow2",
				"rainbowgroup keyword.rainbow2"
			],
			"settings": {
				"foreground": "#004768", /* H=7/12 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow3",
				"rainbowgroup entity.name.function.rainbow3"
			],
			"settings": {
				"foreground": "#685E00", /* H=5/36 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow4",
				"rainbowgroup comment.rainbow4"
			],
			"settings": {
				"foreground": "#270068", /* H=25/36 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow5",
				"rainbowgroup string.rainbow5"
			],
			"settings": {
				"foreground": "#476800", /* H=1/4 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow6",
				"rainbowgroup variable.parameter.rainbow6"
			],
			"settings": {
				"foreground": "#5E0068", /* H=29/36 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow7",
				"rainbowgroup constant.numeric.rainbow7"
			],
			"settings": {
				"foreground": "#006827", /* H=13/36 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow8",
				"rainbowgroup entity.name.type.rainbow8"
			],
			"settings": {
				"foreground": "#680047", /* H=11/12 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow9",
				"rainbowgroup markup.bold.rainbow9"
			],
			"settings": {
				"foreground": "#00685E", /* H=17/36 */
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"rainbowgroup rainbow10",
				"rainbowgroup invalid.rainbow10"
			],
			"settings": {
				"foreground": "#682700", /* H=1/36 */
				"fontStyle": ""
			}
		},
		/* unmodified */
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#000000ff"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#09885a"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#811f3f"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#cd3131"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"punctuation.definition.quote.begin.markdown",
				"punctuation.definition.list.begin.markdown"
			],
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"scope": [
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#a31515"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#811f3f"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"support.constant.handlebars",
				"source.powershell variable.other.member",
			],
			"settings": {
				"foreground": "#795E26"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#267f99"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
			],
			"settings": {
				"foreground": "#267f99"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"meta.definition.variable.name",
			],
			"settings": {
				"foreground": "#001080"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#001080"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#0451a5"
			}
		},
		{
			"name": "Regular expression groups",
			"scope": [
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"keyword.operator.negation.regexp",
				"support.other.parenthesis.regexp"
			],
			"settings": {
				"foreground": "#d16969"
			}
		},
		{
			"scope": "keyword.operator.quantifier.regexp",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
