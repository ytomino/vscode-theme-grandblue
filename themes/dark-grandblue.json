{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dark Grandblue",
	"type": "dark",
	"include": "./include/common.json",
	"colors": {
		/* the drawing order of the background colors:
		 *   editor.background
		 *   editor.lineHighlightBackground
		 *   editor.selectionBackground / editor.inactiveSelectionBackground
		 *   editor.findRangeHighlightBackground
		 *   editor.findMatchBackground / editor.findMatchHighlightBackground
		 *   editor.selectionHighlightBackground
		 *   editorIndentGuide.background */
		"editor.background": "#000020",
		"editor.lineHighlightBackground": "#081030",
		"editorIndentGuide.background": "#102040",
		"editorRuler.foreground": "#142849CC", /* 5/4 of editorIndentGuide.background with 4/5 transparency */
		"editorWhitespace.foreground": "#204060",
		/* line number */
		"editorLineNumber.foreground": "#285070",
		"editorLineNumber.activeForeground": "#387090",
		/* scrollbar(overview ruler) */
		"editorOverviewRuler.border": "#202020CC", /* 4/5 transparency */
		/* selection */
		"editor.selectionBackground": "#003870",
		"editor.inactiveSelectionBackground": "#004860",
		"editor.selectionHighlightBackground": "#00A0C333", /* 5 of #002040 with 1/5 transparency */
		/* finding */
		"editor.findMatchBackground": "#C0002066", /* 5/2 of #600020 with 2/5 transparency */
		"editor.findMatchBorder": "#F00020CC", /* 5/4 of #C00020 with 4/5 transparency */
		"editor.findMatchHighlightBackground": "#F0002033", /* 5 of #300020 with 1/5 transparency */
		"editor.findMatchHighlightBorder": "#F00020CC", /* 5/4 of #C00020 with 4/5transparency */
		/* sidebar */
		"sideBar.background": "#1E1E1E",
		/* panel */
		"panel.background": "#1E1E1E",
		/* explorer */
		"listFilterWidget.background": "#C01E1E66", /* similar as editor.findMatchBackground */
		"listFilterWidget.outline": "#F01E1ECC", /* similar as editor.findMatchBorder */
		"listFilterWidget.noMatchesOutline": "#F01EF0CC"
	}
}
